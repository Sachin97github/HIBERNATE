<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.nit.entity.ProgrammerProjectMgmtInfo"
		table="PROGRAMMER_PROJECT_INFO">
		<composite-id name="id" class="com.nit.entity.ProgrammerProjectMgmtId">
			<key-property name="projId" column="PROJID" />
			<key-property name="pId" column="PID" />
		</composite-id>
		<property name="pName" column="PNAME"/>
		<property name="projName" column="PROJNAME"/>
		<property name="deptName" column="DEPTNAME"/>
		
	</class>

</hibernate-mapping>
	
	
